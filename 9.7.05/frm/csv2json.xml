<DAT name="DETAIL" xml:space='preserve'>variables 
  string vInput, vOutput, vLine, vItem, vHeader, vHeaders, vValue, vDelimiter
  numeric vItemIndex, vLineIndex
  boolean vFirstLineContainsLabels
  struct vStruct, vMember
endvariables 

; ###############################################
; ### Init 
; ###############################################
if (NM.DE = "")
  message "No file was selected. Please place a CSV file in the drop target."
  return -1
endif


; ###############################################
; ### Processing
; ###############################################


; ###############################################
; ### Tbd
; ###############################################

; init 
vFirstLineContainsLabels = DFHEADERS.DE
vOutput = "the JSON structure should be here."

; read the file and find the seperator 
selectcase DFDELIMITER.DE
  case "TAB"
    vDelimiter = "	"
  case "OTHER"
    vDelimiter = DFOTHER.DE
    if (DFOTHER.DE = "") 
      message "When other is selected a value needs to be entered."
      $prompt = "DFOTHER.DE"
      return -1
    endif 

  elsecase
    getitem/id vDelimiter, $valrep(dfdelimiter), dfdelimiter
endselectcase

; read the file 
fileload NM.DE, vInput

; replace the endofline (%%^) characters by a list separator &uSEP;
vInput = $replace(vInput, 1, "%%^", "&uSEP;", -1)


; start get the headers from the first line
getitem vLine, vInput, 1
delitem vInput, 1

; strip the quotes 
call strip(vDelimiter, vLine)

while (vLine != "")
  getitem (vHeader, vLine, 1)
  if (vHeader !="")
	if (vFirstLineContainsLabels)
      putitem vHeaders, -1, vHeader
    else 
      putitem vHeaders, -1, ($itemcount(vHeaders) + 1)
    endif 
  endif 
  delitem vLine, 1
endwhile
; endgetheaders

; process the data lines 
vStruct = $newstruct
forlist vLine, vLineIndex in vInput
   call strip(vDelimiter, vLine)

   ; now step through the headers 
   vMember = $newstruct
   forlist vHeader, vItemIndex in vHeaders
     getItem(vValue, vLine, vItemIndex)
     vMember-&gt;"%%vHeader%%%" = vValue
   endfor   
   vStruct-&gt;"%%vLineIndex%%%" = $newstruct
   vStruct-&gt;"%%vLineIndex%%%" = vMember 
endfor

; Convert a JSON text to a Struct.
;jsonToStruct vStruct, vContent
structToJson JSON.DE, vStruct
call jsonFormatter(JSON.DE)

return 0
done

entry strip
  params 
    string pDelimiter : in
    string pLine : inout
  endparams 

  ; turn the string into a list by replacing the spearator with a [GOLD] + [;]
  pLine = $replace(pLine, 1, pDelimiter, "&uSEP;", -1)
  ; now remove all the remaining quotes
  pLine = $replace(pLine, 1, "%%"", "", -1)
  return 
end ;strip

; format the json string 
entry jsonFormatter
  params 
    string pContent : inout
  endparams 
  variables  
    string vContent, vValue 
    numeric vNumberOfSpaces
  endvariables 

  ; init 
  vNumberOfSpaces = 0

  ; turn it into a list 
  pContent = $replace(pContent, 1, "{", "{&uSEP;", -1)
  pContent = $replace(pContent, 1, "}}", "&uSEP;}&uSEP;}", -1)
  pContent = $replace(pContent, 1, "},", "&uSEP;},&uSEP;", -1)
  pContent = $replace(pContent, 1, "%%",", "%%",&uSEP;", -1)

  pContent = $replace(pContent, 1, "&uSEP;&uSEP;", "&uSEP;", -1)


  forlist vValue in pContent
    call getIdentation(vValue, vNumberOfSpaces, 2)

    vContent = $concat(vContent, vValue, "%%^")
  endfor 
  pContent = vContent

  return 0
end 

entry getIdentation
  params
    string pLine : inout
    numeric pCounter : inout
    numeric pStepsize : in
  endparams 
  variables 
    numeric vPointer 
  endvariables 

  ; subtract spacers
  if ($scan(pLine, "}") &gt; 0) 
    pCounter -= pStepsize
  endif 

  vPointer = 0
  while (vPointer &lt; pCounter)
    pLine = $concat(" ", pLine)
    vPointer += 1
  endwhile

  ; add spacers
  if ($scan(pLine, "{") &gt; 0)
    pCounter += pStepsize
  endif 

  return 0
end</DAT>
