<DAT name="ULABEL">GETAGE</DAT>
<DAT name="ULAN">P</DAT>
<DAT name="UDESCR" xml:space='preserve'>Get the age</DAT>
<DAT name="UTEXT" xml:space='preserve'>; @description = pReturnType is either Centuries, Year, Months, Weeks, Days, Hours, Minutes, Seconds, Tics
; pDate1 is the first date 
; pDate2 is the second date
; if one of the dates is empty we assume you want to compare it to the current date.
; the result will always be a positive number 
entry getAge
   returns numeric
   params 
      date pDate1 : in 
      date pDate2 : in 
      string pReturnType : in
   endparams
   variables 
      numeric vResult
      date vDate
      string vList
   endvariables 

   ; init 
   vList  = "C&uSEP;Y&uSEP;M&uSEP;D&uSEP;HRS&uSEP;MIN&uSEP;SEC&uSEP;TIC"
   if (pDate1 = "" &amp; pDate2 = "")
      message "getAge: Unable to determine date."
      return (0)
   endif
   if (pReturnType = "")
      ; assume years 
      pReturnType = "Y"
   else
      pReturnType = $uppercase(pReturnType)
      if ($item(pReturnType, vList) != pReturnType)
         message "getAge: return type not recognized."
         return (0)
      endif 
   endif 
   ; if one of the dates is empty fill it with the current date
   if (pDate1 = "")
      pDate1 = $datim
   endif 
   if (pDate2 = "")
      pDate2 = $datim
   endif
   ; now make sure the first date is the one farthest away
   if (pDate1 &gt; pDate2)
      vDate = pDate1
      pDate1 = pDate2
      pDate2 = vDate
   endif 

   ; do the actual processing
   selectcase pReturnType
   case "C"
      ; works
      vResult = $abs(pDate1[Y] - pDate2[Y])/100
   case "Y"
      ; works
      vResult = $abs(pDate1[Y] - pDate2[Y])
   case "M"
      ; works
      ; get the number of years and multiply them by 12
      debug
      vResult = (getAge(pDate1, pDate2, "Y") -1)* 12
      vResult += 12 - pDate1[M]
      vResult += pDate2[M]
   case "D"
      vResult = $abs(pDate1 - pDate2)
   case "HRS"
   case "MIN"
   case "SEC"
   case "TIC"
   endselectcase
   vResult = vResult[round]
 
   return vResult
end ; getAge
</DAT>
