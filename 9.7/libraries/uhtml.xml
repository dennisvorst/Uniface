<DAT name="ULABEL">CREATEHTML</DAT>
<DAT name="ULAN">P</DAT>
<DAT name="UDESCR" xml:space='preserve'>Create a html string</DAT>
<DAT name="UTEXT" xml:space='preserve'>entry createHtml
   returns string
   params
      string pHead : in
      string pBody : in
   endparams 

   return "&lt;html&gt;%%^&lt;head&gt;%%^%%%pHead%%%%^&lt;/head&gt;%%^&lt;body&gt;%%^%%%pBody%%&lt;/body&gt;%%^&lt;/html&gt;%%^"
end ; createHtml</DAT>
</OCC>
<OCC>
<DAT name="UTIMESTAMP">2014-10-20T19:58:51.54</DAT>
<DAT name="USUB">I</DAT>
<DAT name="UVAR">UHTML</DAT>
<DAT name="ULABEL">CREATEINPUTFIELD</DAT>
<DAT name="ULAN">P</DAT>
<DAT name="UDESCR" xml:space='preserve'>Create input element</DAT>
<DAT name="UTEXT" xml:space='preserve'>entry createInputField
   ; @description = create an HTML input element
   returns string
   params
      string pNmid      : in ; the css ID value
      string pNmclass   : in ; the HTML classname
      string pNmelement : in ; the name of the widget
      string pNmtype    : in ; the type, see vFtsupported for supported types
      string pFtlist    : in ; List with possible selections, list item stuff only
      string pNmdefault : in ; default value 
   endparams
   variables 
      string vFtresult, vFtsupported
   endvariables 

   ; init 
   vFtsupported = "button•;checkbox•;color•;date•;datetime•;datetime-local•;email•;file•;hidden•;image•;month•;number•;password•;radio•;range•;reset•;search•;submit•;tel•;text•;time•;url•;week"

   if (pNmelement = "" || $item(vFtsupported, vNmtype) != pNmtype)
      message "createInputElement: Unable to create element."
      return 0
   endif 

   if (pNmid != "")
      vFtresult += "name='%%pNmid%%%' "
   endif 
   if (pNmclass != "")
      vFtresult += "class='%%pNmclass%%%' "
   endif 
   if (pNmtype != "")
      vFtresult += "type='%%pNmtype%%%' "
   endif 
   if (pNmelement != "")
      vFtresult += "name='%%pNmelement%%%' "
   endif 
   if (pNmdefault != "")
      vFtresult += "value='%%pNmdefault%%%' "
   endif 

   vFtresult = $concat("&lt;input", vFtresult, "&gt;")

   return vFtresult
end ; createElement
</DAT>
