<DAT name="VCHANGED" xml:space='preserve'>variables
   string vFileName, vFileType, vInput, vLine
   numeric vNumberOfColumns, vLineIndex
   string vNumberOfSemiColons, vNumberOfSemiCommas, vNumberOfSemiTabs, vListTabs, vListCommas, vListSemiColons
endvariables

; only one csv file is allowed
getitem vFileType, &lt;$fieldname&gt;.&lt;$entname&gt;, 1
getitem vFileName, &lt;$fieldname&gt;.&lt;$entname&gt;, 2

if (vFileType != "file") 
  ; if the type is not a file
  message "The element you dropped was of a type %%vFileType%%%. Please provide a file."
  return -1
elseif ($uppercase (ftimage[$length(ftimage) - 2]) != "CSV") 
  ; if the file name doesnot end with CSV reject it 
  message "Please provide a file of type CSV and with the CSV extention."
  return -1
else 
  ; set the filename
  NM.DE = vFileName
endif 

; reset the background image
&lt;$fieldname&gt;.&lt;$entname&gt; = "image"


; ###############################################
; ### Preprocessing checks
; ###############################################
fileload NM.DE, vInput
if ($status &lt; 0)
  message "The file failed to load correctly. Make sure it isn't opened and try again."
  return -1
endif 

; remove all the trailing enters at the end
vInput = $rtrim(vInput, "%%^")

; replace the %%^ with &uSEP;
vInput = $replace(vInput, 1, "%%^", "&uSEP;", -1)

; find the seperator 
forlist vLine, vLineIndex in vInput
  vNumberOfSemiColons = countElementsInString(";", vLine)
  vNumberOfSemiCommas = countElementsInString(",", vLine)
  vNumberOfSemiTabs = countElementsInString("	", vLine)
 
  putitem vListTabs, -1, vNumberOfSemiTabs
  putitem vListCommas, -1, vNumberOfSemiCommas 
  putitem vListSemicolons, -1, vNumberOfSemiColons 
endfor

sort/list vListTabs, "U"
sort/list vListCommas, "U"
sort/list vListSemicolons, "U"

; here is the tricky part. 
; one has to have a value larger than zero and only one occurence
; while the others should be either 0 all the way trhough or have irregular values (so menaing a count other than 1)
; set the DELIMITER field 
if (vListTabs &gt; 0)
  DFDELIMITER.DE = "TAB"
elseif (vListCommas &gt; 0)
  DFDELIMITER.DE = "COMMA"
elseif (vListSemicolons &gt; 0)
  DFDELIMITER.DE = "SEMICOLON"
else 
  message "Could not uniquely identify the separator. You have to set it manually."
endif

return 0
done

entry countElementsInString
  returns numeric
  params 
    string pNeedle : in 
    string pHaystack : in
  endparams 
  variables 
    numeric vPos, vResult, vNeedleLength
  endvariables 

  ; init
  vResult = 0
  vNeedleLength = $length(pNeedle)

  vPos = $scan(pHaystack, pNeedle)

  while (vPos != 0)
    if (vPos &gt; 0)
      pHaystack = pHaystack[vPos + vNeedleLength]
      vResult += 1
    endif
    vPos = $scan(pHaystack, pNeedle)
  endwhile  

  return vResult
end

